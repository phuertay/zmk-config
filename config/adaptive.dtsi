/ { behaviors {
	slad: adaptive_l {
		compatible = "zmk,behavior-sticky-key";
		label = "SL_AL";
		quick-release;
		#binding-cells = <1>;
		release-after-ms = <ADAPTIVE_DELAY>;
		bindings = <&mo>;
	};

	ZMK_MACRO_F(ak_Q,
		bindings = <&slad AL_Q &kp Q>;
	)

	ZMK_MACRO_F(ak_W,
		bindings = <&slad AL_W &kp W>;
	)

	ZMK_MACRO_F(ak_E,
		bindings = <&slad AL_E &kp E>;
	)

	ZMK_MACRO_F(ak_R,
		bindings = <&slad AL_R &kp R>;
	)

	ZMK_MACRO_F(ak_O,
		bindings = <&slad AL_O &kp O>;
	)

	ZMK_MACRO_F(ak_P,
		bindings = <&slad AL_P &kp P>;
	)

	ZMK_MACRO_F(ak_SEMI,
		bindings = <&slad AL_SEMI &kp SEMI>;
	)

	ZMK_MACRO_F(ak_Z,
		bindings = <&slad AL_Z &kp Z>;
	)

	ZMK_MACRO_F(ak_X,
		bindings = <&slad AL_X &kp X>;
	)

	ZMK_MACRO_F(ak_C,
		bindings = <&slad AL_C &kp C>;
	)

	ZMK_MACRO_F(ak_M,
		bindings = <&slad AL_M &kp M>;
	)

	ZMK_MACRO_F(ak_FSLH,
		bindings = <&slad AL_FSLH &kp FSLH>;
	)

	ZMK_MACRO_F(ak_DOT,
		bindings = <&slad AL_DOT &kp DOT>;
	)

	ZMK_MACRO_F(m_dba,
		bindings = <&kp BSPC &kp B &kp A>;
	)

	ZMK_MACRO_F(m_dbc,
		bindings = <&kp BSPC &kp B &kp C>;
	)

	ZMK_MACRO_F(m_ddw,
		bindings = <&kp BSPC &kp D &kp W>;
	)

	ZMK_MACRO_F(m_djm,
		bindings = <&kp BSPC &kp J &kp M>;
	)

	ZMK_MACRO_F(m_dge,
		bindings = <&kp BSPC &kp G &kp E>;
	)

	ZMK_MACRO_F(m_dgc,
		bindings = <&kp BSPC &kp G &kp C>;
	)

	ZMK_MACRO_F(m_dvy,
		bindings = <&kp BSPC &kp V &kp Y>;
	)

	ZMK_MACRO_F(m_dga,
		bindings = <&kp BSPC &kp G &kp A>;
	)

	ZMK_MACRO_F(m_dg_gr,
		bindings = <&kp BSPC &kp G &kp GRAVE>;
	)

	ZMK_MACRO_F(m_dkk,
		bindings = <&kp BSPC &kp Y>;
	)

	ZMK_MACRO_F(m_dqt,
		bindings = <&kp BSPC &kp SQT>;
	)

	ZMK_MACRO_F(m_mk,
		bindings = <&kp M &kp K>;
	)

	hms_m_dot: homerow_mod_w_macro {
		compatible = "zmk,behavior-hold-tap";
		label = "HOMEROW_MOD_SLOW_W_MACRO";
		#binding-cells = <2>;
		tapping-term-ms = <220>;
		quick-tap-ms = <0>;
		flavor = "balanced";
		bindings = <&kp>, <&ak_DOT>;
	};

	hms_m_m: homerow_mod_m_macro {
		compatible = "zmk,behavior-hold-tap";
		label = "HOMEROW_MOD_SLOW_M_MACRO";
		#binding-cells = <2>;
		tapping-term-ms = <220>;
		quick-tap-ms = <0>;
		flavor = "balanced";
		bindings = <&kp>, <&ak_M>;
	};

	hms_m_x: homerow_mod_x_macro {
		compatible = "zmk,behavior-hold-tap";
		label = "HOMEROW_MOD_SLOW_X_MACRO";
		#binding-cells = <2>;
		tapping-term-ms = <220>;
		quick-tap-ms = <0>;
		flavor = "balanced";
		bindings = <&kp>, <&ak_X>;
	};
	
	hm_m_c: homerow_mod_c_macro {
		compatible = "zmk,behavior-hold-tap";
		label = "HOMEROW_MOD_SLOW_C_MACRO";
		#binding-cells = <2>;
		tapping-term-ms = <166>;
		quick-tap-ms = <0>;
		flavor = "balanced";
		bindings = <&kp>, <&ak_C>;
	};
}; };
